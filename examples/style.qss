/* Button styles */
#myButton {
    color: red;
    font: bold 14px;
}
QPushButton[data-value="complex string with spaces"] {
    background: blue;
    border: 2px solid white;
}
QPushButton:hover {
    background: darkblue;
}

/* Label styles */
#myLabel {
    color: green;
    font: 12pt "Arial";
}
QLabel {
    background: lightgray;
}

/* Custom widget styles */
QWidget[theme="dark"] {
    background: #333333;
    border-radius: 5px;
}
.customClass {
    border: 1px dashed yellow;
}